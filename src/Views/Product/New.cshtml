@model  DNA.Example.ProductCatalog.Models.Product
@{
    Layout = "~/views/shared/_dashboard.cshtml";
    ViewBag.Title = "New product";
    ViewBag.ReturnUrl = Url.SolutionAction("ProductCatalog", "List", "Product", Culture, WebName);
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    Html.RenderPartial("~/Views/Product/_Form.cshtml");
    <div style="text-align:center;">
        <button data-default="true" data-inline="false">@Resources.Commons.Save</button>
    </div>
}